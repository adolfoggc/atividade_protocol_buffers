<p id="notice"><%= notice %></p>

<h1>Room Statuses</h1>

<table>
  <thead>
    <tr>
      <th>Ac temperature</th>
      <th>Ac status</th>
      <th>Tv status</th>
      <th>Tv channel</th>
      <th>Lamp status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @room_statuses.each do |room_status| %>
      <tr>
        <td><%= room_status.ac_temperature %></td>
        <td><%= room_status.ac_status %></td>
        <td><%= room_status.tv_status %></td>
        <td><%= room_status.tv_channel %></td>
        <td><%= room_status.lamp_status %></td>
        <td><%= link_to 'Show', room_status %></td>
        <td><%= link_to 'Edit', edit_room_status_path(room_status) %></td>
        <td><%= link_to 'Destroy', room_status, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Room Status', new_room_status_path %>
